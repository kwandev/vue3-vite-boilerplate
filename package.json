{
  "name": "vue3-vite-boilerplate",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "debug": "vite --mode debug",
    "dev": "vite --mode dev",
    "staging": "vite --mode staging",
    "prod": "vite --mode prod",
    "build:dev": "vue-tsc --noEmit && vite build --mode dev",
    "build:staging": "vue-tsc --noEmit && vite build --mode staging",
    "build:prod": "vue-tsc --noEmit && vite build --mode prod",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@vueuse/core": "^8.3.1",
    "axios": "^0.27.1",
    "dayjs": "^1.11.1",
    "lodash": "^4.17.21",
    "pinia": "^2.0.13",
    "uuid": "^8.3.2",
    "vue": "^3.2.25",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.182",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^8.7.1",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.5",
    "vite-plugin-eslint": "^1.6.0",
    "vue-tsc": "^0.34.7"
  }
}
